 code=>'my @a=({"larry"=>2,"moe"=>1,"curly"=>3},{"groucho"=>1,"harpo"=>3,"chico"=>2},{"seagoon"=>2,"bloodnok"=>1,"eccles"=>3}); GraphViz::Data::Structure->new(\@a,%title%)->graph->as_canon',
)
